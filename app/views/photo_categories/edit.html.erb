<h1>Editing photo_category</h1>

<% form_for(@photo_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @photo_category %> |
<%= link_to 'Back', photo_categories_path %>
